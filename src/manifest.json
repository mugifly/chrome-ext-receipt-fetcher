{
  "manifest_version": 3,
  "name": "receipt-fetcher",
  "description": "",
  "version": "1.0.0",
  "icons": {},
  "permissions": ["tabs", "windows", "scripting"],
  "host_permissions": [],
  "action": {
    "default_title": "ReceiptFetcher"
  },
  "background": {
    "service_worker": "background-script.js"
  },
  "options_ui": {
    "page": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://mugifly.github.io/*",
        "https://*.esa.io/*",
        "https://*.heroku.com/*"
      ],
      "all_frames": true,
      "css": [],
      "js": ["content-script.js"]
    }
  ]
}
